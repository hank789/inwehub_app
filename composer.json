{
    "name": "tipask/tipask",
    "description": "PHP社区问答系统",
    "keywords": ["问答系统", "tipask","laravel"],
    "license": "MPL",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "bizly/roles": "^2.1",
        "doctrine/dbal": "~2.3",
        "intervention/image": "^2.3",
        "jacobcyl/ali-oss-storage": "dev-master",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "mews/captcha": "~2.0",
        "mews/purifier": "^2.0",
        "predis/predis": "^1.1",
        "roumen/sitemap": "^2.6",
        "sentry/sentry-laravel": "^0.6.1",
        "socialiteproviders/qq": "^3.0",
        "socialiteproviders/weibo": "^3.0",
        "socialiteproviders/weixin": "^4.0",
        "socialiteproviders/weixin-web": "^3.0",
        "tlapi/slack-laravel": "dev-master",
        "zhuzhichao/ip-location-zh": "^2.2",
        "tymon/jwt-auth": "^0.5.10",
        "barryvdh/laravel-cors": "^0.9.2",
        "flc/alidayu": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "barryvdh/laravel-ide-helper":"dev-master",
        "barryvdh/laravel-debugbar": "^2.1",
        "laravel/browser-kit-testing": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Tlapi/slack-laravel.git"
        },
        {
            "packagist": false
        }
    ]
}
