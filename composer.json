{
    "name": "inwehub/inwehub",
    "description": "",
    "keywords": ["问答系统", "laravel"],
    "license": "MPL",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "barryvdh/laravel-cors": "^0.9.2",
        "barryvdh/laravel-snappy": "^0.4.1",
        "bizly/roles": "^2.1",
        "doctrine/dbal": "~2.3",
        "elasticsearch/elasticsearch": "^6.0",
        "fabpot/goutte": "^3.2",
        "flc/alidayu": "^2.0",
        "h4cc/wkhtmltoimage-amd64": "^0.12.4",
        "hexianghui/bosonnlp-sdk": "^1.0",
        "intervention/image": "^2.3",
        "jacobcyl/ali-oss-storage": "dev-master",
        "jaeger/querylist": "^4.0",
        "jaeger/querylist-phantomjs": "^4.0",
        "laravel/framework": "5.5.*",
        "laravel/scout": "^4.0",
        "laravel/tinker": "~1.0",
        "lucadello91/laravel-scout-elasticsearch": "^0.1.3",
        "maatwebsite/excel": "~2.1.0",
        "mews/captcha": "~2.0",
        "mews/purifier": "^2.0",
        "mixpanel/mixpanel-php": "^2.6",
        "overtrue/laravel-pinyin": "~3.0",
        "overtrue/wechat": "~3.0",
        "paquettg/php-html-parser": "^1.7",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.6",
        "riverslei/payment": "dev-master",
        "sentry/sentry-laravel": "^0.8.0",
        "spatie/laravel-backup": "5.*",
        "spatie/laravel-medialibrary": "6.*",
        "symfony/filesystem": "^3.3",
        "tightenco/collect": "^5.6",
        "tlapi/slack-laravel": "dev-master",
        "tymon/jwt-auth": "^0.5.10",
        "zhuzhichao/ip-location-zh": "^2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "barryvdh/laravel-ide-helper":"2.4.*",
        "barryvdh/laravel-debugbar": "^2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Tlapi/slack-laravel.git"
        },
        {
            "packagist": false
        }
    ]
}
